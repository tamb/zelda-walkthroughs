{
  "name": "zelda-walkthroughs",
  "version": "1.0.0-alpha.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "build:gh-pages": "GITHUB_PAGES=true REPOSITORY_NAME=zelda-walkthroughs rsbuild build",
    "build:clean": "node scripts/clear-cache.js && npm run build:gh-pages",
    "check": "biome check --write",
    "deploy": "npm run build:gh-pages && gh-pages -d dist",
    "deploy:script": "node scripts/deploy.js",
    "dev": "rsbuild dev --open",
    "format": "biome format --write",
    "generate:icons": "node scripts/svg-to-png.js",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "localforage": "^1.10.0",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@rsbuild/core": "^1.5.6",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "gh-pages": "^6.3.0",
    "sass": "^1.93.2",
    "sharp": "^0.34.4",
    "typescript": "^5.9.2"
  }
}
